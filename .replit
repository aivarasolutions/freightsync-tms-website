modules = ["web", "nodejs-20"]
[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33521
externalPort = 3002

[[ports]]
localPort = 34869
externalPort = 3000

[[ports]]
localPort = 43377
externalPort = 3001

[deployment]
deploymentTarget = "autoscale"
run = ["npm"]
build = ["npm", "run", "build"]
